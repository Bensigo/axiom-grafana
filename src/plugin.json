{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "Axiom",
  "id": "axiom-grafana-datasource",
  "metrics": true,
  "logs": false,
  "backend": true,
  "executable": "gpx_my_plugin",
  "info": {
    "description": "Query axiom through grafana",
    "author": {
      "name": "Axiom"
    },
    "keywords": ["datasource"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": "^9.3.8",
    "plugins": []
  },
  "routes": [
    {
      "path": "datasets",
      "method": "POST",
      "url": "{{ .JsonData.apiHost }}/v1/datasets",
      "reqRole": "Editor",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{ .SecureJsonData.accessToken }}"
        },
        {
          "name": "UserAgent",
          "content": "axiom-grafana/v1.0.0"
        },
        {
          "name": "Content-Type",
          "content": "application/json"
        }
      ]
    }
  ]
}
